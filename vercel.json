{
  "version": 2,
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "regions": ["pdx1"],
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "NEXT_PRIVATE_TARGET": "server"
    }
  }
}
